{
  "name": "JSPass",
  "version": "1.4",
  "manifest_version": 2,
  "description": "Autofill generated random passwords using PBKDF2 from http://crypto-js.googlecode.com. Icon comes from http://www.proicons.com.",
  "options_page": "options.html",
  "icons": { 
    "16": "icon16.png",
    "48": "icon48.png",
    "128": "icon128.png" 
  },
  "background": {
    "scripts": ["jquery-1.8.2.min.js", "background.js"],
    "persistent": false
  },
  "browser_action": {
    "default_icon": "icon19.png",
    "default_title": "JSPass",
    "default_popup": "popup.html"
  },
  "permissions": [
    "tabs",
    "clipboardWrite",
    "storage",
    "http://*/*",
    "https://*/*"
  ]
}